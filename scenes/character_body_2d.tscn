script/source = "extends CharacterBody2D

# Constants and variables
const SPEED = 300.0
const JUMP_VELOCITY = -400.0
var gravity = ProjectSettings.get_setting(\"physics/2d/default_gravity\")

# Reference to the AnimatedSprite2D node
@onready var sprite = $AnimatedSprite2D

func _ready():
	# Check if the sprite node exists
	if sprite == null:
		print(\"Error: AnimatedSprite2D node not found. Please check the path.\")
	else:
		print(\"AnimatedSprite2D node found successfully.\")

func _physics_process(delta):
	# Add the gravity.
	if not is_on_floor():
		velocity.y += gravity * delta

	# Handle jump.
	if Input.is_action_just_pressed(\"JUMP\") and is_on_floor():
		velocity.y = JUMP_VELOCITY

	# Get the input direction and handle the movement/deceleration.
	var direction = Input.get_axis(\"LEFT\", \"RIGHT\")
	if direction != 0:
		velocity.x = direction * SPEED
		# Flip the sprite based on the direction
		sprite.scale.x = -1 if direction < 0 else 1
		
		# Play the walking animation
		if not sprite.is_playing() or sprite.animation != \"WALK\":
			sprite.play(\"WALK\")
	else:
		velocity.x = move_toward(velocity.x, 0, SPEED)
		
		# Stop the walking animation and play idle if not moving
		if sprite.is_playing() and sprite.animation == \"WALK\":
			sprite.stop()
		if velocity.x == 0:
			sprite.play(\"IDLE\")

	move_and_slide()
"

